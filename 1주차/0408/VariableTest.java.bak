class VariableTest
{
	public static void main(String[] args) 
	{
		System.out.println("변수 안녕");

		System.out.println("출력하고자 하는 데이터~!");

		System.out.println(20);

		int su;		//변수 선언
		su = 30;	//변수 초기화(값 할당)

		System.out.println(su);
		System.out.println("======");

